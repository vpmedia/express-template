{
  "name": "@vpmedia/express-template",
  "version": "1.0.0",
  "description": "Express Template",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/vpmedia/express-template#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/express-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/express-template.git"
  },
  "keywords": [],
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "bin": {
    "server": "./src/index.js"
  },
  "scripts": {
    "start": "./src/index.js",
    "watch": "nodemon",
    "prebuild": "rm -rf ./build && rm -rf ./lib",
    "build": "rollup -c",
    "postbuild": "chmod +x ./build/server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint-fix": "eslint \"**/*.{js,ts}\" --fix",
    "typecheck": "tsc",
    "prepack": "npm run build",
    "lefthook:install": "lefthook install",
    "lefthook:uninstall": "lefthook uninstall"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.48.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.48.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.48.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.48.0",
    "@opentelemetry/instrumentation-express": "^0.35.0",
    "@opentelemetry/instrumentation-http": "^0.48.0",
    "@opentelemetry/resources": "^1.21.0",
    "@opentelemetry/sdk-metrics": "^1.21.0",
    "@opentelemetry/sdk-node": "^0.48.0",
    "@opentelemetry/sdk-trace-node": "^1.21.0",
    "@opentelemetry/semantic-conventions": "^1.21.0",
    "@rollup/plugin-json": "^6.1.0",
    "@sentry/node": "^7.99.0",
    "@wesleytodd/openapi": "^0.3.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.0",
    "redis": "^4.6.12",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.12",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsdoc": "^48.0.4",
    "jest": "^29.7.0",
    "lefthook": "^1.6.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "rollup": "^4.9.6",
    "typescript": "^5.3.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  }
}
